<div class="untree_co-section pt-3">
  <div class="">

    <div class="row align-items-center mb-5">
      <div class="col-lg-8">
        <h2 class="mb-3 mb-lg-0">{{{title}}}</h2>
      </div>
      <div class="col-lg-4">

        {{!-- <div class="d-flex pt-4 align-items-center justify-content-lg-end">
          <div class="dropdown bg-transparent">
              <button class="btn btn-secondary dropdown-toggle bg-transparent text-dark" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                Price
              </button>
              <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <a class="dropdown-item" href="/customer/shop-single/price-asc">Ascending</a>
                <a class="dropdown-item" href="/customer/shop-single/price-dec">Decending</a>
                <a class="dropdown-item" href="/customer/shop-single/price-50">Under $50</a>
                <a class="dropdown-item" href="/customer/shop-single/price-100">Under $100</a>
                <a class="dropdown-item" href="/customer/shop-single/price-300">Under $300</a>
              </div>
            </div>
        </div> --}}
      </div>
    </div>

    <div class="row">
      <div class="col-md-3 w-100 d-flex justify-content-between mb-5">
        <div class="bg-transparent">
            <form method="POST" action="/admin/product-report/time">
              <div class="row">
                <h5>Filtered by time</h5>
                <div class="col-md-5 mb-4 me-4">

                  <div class="form-outline">
                    <input type="date" id="startDate" class="form-control form-control-lg bg-light today" name="startDate" required/>
                    <label class="form-label" for="startDate">Start</label>
                  </div>

                </div>
                <div class="col-md-5 mb-4">

                  <div class="form-outline">
                    <input type="date" id="endDate" class="form-control form-control-lg bg-light tomorrow" name="endDate" required/>
                    <label class="form-label" for="endDate">End</label>
                  </div>

                </div>
                <button type="submit" class="col-md-6 btn btn-info" >Apply filtered</button>
              </div>
            </form>
        </div>
      </div>
      <table class="table align-items-center mb-0">
                <thead>
                    <tr>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Name</th>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Price</th>
                      <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Category</th>
                      <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Total orders</th>
                      <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Total sold</th>
                      <th class="text-uppercase text-center text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Revenue</th>
                    </tr>
                  </thead>
                  <tbody>
                    {{#each productReports}}
                      <tr>
                        <td>
                          <div class="d-flex px-2 py-1">
                            <div class="d-flex flex-column justify-content-center">
                              <h6 class="mb-0 text-sm">{{this.name}}</h6>
                            </div>
                          </div>
                        </td>
                        <td>
                          <p class="text-xs font-weight-bold mb-0">${{this.price}}</p>
                        </td>
                        <td class="align-middle text-center">
                          {{#each this.category}}
                            <p class="text-xs font-weight-bold">{{this}}</p>
                          {{/each}}
                        </td>
                        <td class="text-center">
                          <span class="text-secondary text-xs font-weight-bold">{{this.totalOrders}}</span>
                        </td>
                        <td class="text-center">
                          <span class="text-secondary text-xs font-weight-bold">{{this.totalOrdered}}</span>
                        </td>
                        <td class="text-center">
                          <span class="text-secondary text-xs font-weight-bold">${{this.revenue}}</span>
                        </td>
                      </tr>
                    {{/each}}
                </tbody>
            </table>
    </div>
  </div> 
</div>

<script>
  let currDate = new Date();
  let tomDate = new Date();
  tomDate.setDate(tomDate.getDate() + 1);

  document.querySelector('input.today').valueAsDate = currDate;
  document.querySelector('input.today').max = currDate.toJSON().split('T')[0];;

  document.querySelector('input.tomorrow').valueAsDate = tomDate;
  document.querySelector('input.tomorrow').min = tomDate.toJSON().split('T')[0];;
</script>